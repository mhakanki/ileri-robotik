// MEHMET HAKAN KIROĞLU
// https://www.youtube.com/@mhakanki
// 1. Dönem Zümre Başkanı Deneyap Teknoloji Atölyeleri Eğitmeni

int led = D0;

void setup() {

  pinMode (led, OUTPUT);
  
  }
            
void loop() {

  digitalWrite (led, HIGH);
  delay (1000);
  digitalWrite (led, LOW);
  delay (1000);
  
  }
